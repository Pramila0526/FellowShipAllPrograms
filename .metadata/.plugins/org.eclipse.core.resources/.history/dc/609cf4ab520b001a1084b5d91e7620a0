package com.bridgelabz.fellowshipprogramss.datastructure;

public class Hashing2
{
	
  	OrderedList[] order=new OrderedList[11];
  	FileRW nu=new FileRW();

  	public  void HashingFunction()
  	{
		int[] retArray = nu.FileReader();

		for(int i=0; i<retArray.length; i++)
		{
			int rem = retArray[i]%11;
			if(order[rem]== null)
			{
				order[rem] = new OrderedList();
				order[rem].insert(retArray[i]);
			}
			else
			{
				order[rem].insert(retArray[i]);
            }
		}
		for(int i=0;i<11;i++)
		{
			System.out.println(i);
			if(order[i] !=null)
			{
				order[i].display();
			}
		}
	}

    public static void main(String[] args)
    {
      	Hashing2 hs=new Hashing2();
    }
}