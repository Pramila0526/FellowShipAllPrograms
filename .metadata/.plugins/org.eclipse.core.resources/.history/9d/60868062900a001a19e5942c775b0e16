import com.bridgelabz.fellowshipprogramss.utility.DataStructure;

public class Filerw 
{

	public static void main(String[] args) throws IOException
	{
		Scanner sc=new Scanner(System.in);
			File file = new File("/home/admin94/Desktop/Text");
			if (!file.exists()) {
				file.createNewFile();
			}
			FileWriter writer = new FileWriter(file); 
			// Writes the content to the fileSystem.out.println("Content");
			String x=sc.nextLine();
			//writer.write(DataStructure.LinkedList.search(x)); 
			DataStructure.search(x);
			writer.flush();
			writer.close();
		}
	}

	
